  "main": "dist/index.js",
  "scripts": {
    "start": "start-storybook -p 3000",
    "build-storybook": "build-storybook",
    "lint": "eslint .",
    "lint-watch": "esw . --watch --clear --fix",
    "flow-watch": "flow-watch --ignore node_modules/ --watch *.js --watch .flowconfig",
